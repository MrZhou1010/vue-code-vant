{
	"name": "vue-code-vant",
	"version": "1.0.0",
	"description": "collection of vant",
	"author": "vant",
	"private": true,
	"scripts": {
		"start": "npm run serve",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"core-js": "^2.6.5",
		"vant": "^2.12.44",
		"vue": "^2.6.10",
		"vue-router": "^3.0.2"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.1.1",
		"@vue/cli-plugin-eslint": "^4.1.1",
		"@vue/cli-service": "^4.1.1",
		"babel-eslint": "^10.0.3",
		"babel-plugin-import": "^1.13.3",
		"eslint": "^4.19.1",
		"eslint-plugin-vue": "^6.0.1",
		"less": "^3.8.1",
		"less-loader": "^5.0.0",
		"postcss-pxtorem": "^6.0.0",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"no-console": "off"
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"Android >= 4.0",
		"iOS >= 8"
	]
}
